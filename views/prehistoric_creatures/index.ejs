<h2>Prehistoric Creatures</h2>

<form method="GET" action="/prehistoric-creatures">
    <label for="nameFilter">Filter by Name</label>
    <input type="text" id="nameFilter" name="nameFilter">
    <input type="submit">
</form>

<ul>
    <% creatures.forEach(creature =>{ %>
        <li>Type: <%= creature.type %> </li>
    <% }) %>
</ul>